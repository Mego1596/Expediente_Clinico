<form method="post" action="{{ path('cita_delete', {'expediente': expediente.getId(),'id': citum.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ citum.id) }}">
    <button class="btn">Delete</button>
</form>
