{% extends 'base.html.twig' %}

{% block title %}Expediente{% endblock %}

{% block body %}
    <h1>Expediente</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ expediente.id }}</td>
            </tr>
            <tr>
                <th>NumeroExpediente</th>
                <td>{{ expediente.numeroExpediente }}</td>
            </tr>
            <tr>
                <th>FechaNacimiento</th>
                <td>{{ expediente.fechaNacimiento ? expediente.fechaNacimiento|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ expediente.direccion }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ expediente.telefono }}</td>
            </tr>
            <tr>
                <th>ApellidoCasada</th>
                <td>{{ expediente.apellidoCasada }}</td>
            </tr>
            <tr>
                <th>EstadoCivil</th>
                <td>{{ expediente.estadoCivil }}</td>
            </tr>
            <tr>
                <th>Creado_en</th>
                <td>{{ expediente.creadoEn ? expediente.creadoEn|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Actualizado_en</th>
                <td>{{ expediente.actualizadoEn ? expediente.actualizadoEn|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('expediente_index') }}">back to list</a>

    <a href="{{ path('expediente_edit', {'id': expediente.id}) }}">edit</a>

    {{ include('expediente/_delete_form.html.twig') }}
{% endblock %}
